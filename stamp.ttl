                                                                                 owl:someValuesFrom :SafetyConstraintProcess
                                                                               ]
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty :requestsToHold ;
                                                                                 owl:someValuesFrom [ owl:intersectionOf ( <http://www.owlsai.com/ontologies/situation#Fluent>
                                                                                                                           [ rdf:type owl:Restriction ;
                                                                                                                             owl:onProperty <http://www.owlsai.com/ontologies/situation#holdsIn> ;
                                                                                                                             owl:someValuesFrom :SafetyConstraintSituation
                                                                                                                           ]
                                                                                                                         ) ;
                                                                                                      rdf:type owl:Class
                                                                                                    ]
                                                                               ]
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty :requestsToNotHold ;
                                                                                 owl:someValuesFrom [ owl:intersectionOf ( <http://www.owlsai.com/ontologies/situation#Fluent>
                                                                                                                           [ rdf:type owl:Class ;
                                                                                                                             owl:complementOf [ rdf:type owl:Restriction ;
                                                                                                                                                owl:onProperty <http://www.owlsai.com/ontologies/situation#holdsIn> ;
                                                                                                                                                owl:someValuesFrom :SafetyConstraintSituation
                                                                                                                                              ]
                                                                                                                           ]
                                                                                                                         ) ;
                                                                                                      rdf:type owl:Class
                                                                                                    ]
                                                                               ]
                                                                             )
                                                               ] ;
                                               rdfs:comment "A Control Action that requests some Actuation that causes some Fluent to not hold that holds in a Hazard"@en ;
                                               rdfs:label "Not Providing Potentially Hazardous Control Action"@en .


###  http://www.owlsai.com/ontologies/stamp#PotentiallyHazardousControlAction
:PotentiallyHazardousControlAction rdf:type owl:Class ;
                                   owl:equivalentClass [ owl:intersectionOf ( <http://www.owlsai.com/ontologies/controlsystem#ControlAction>
                                                                              [ rdf:type owl:Class ;
