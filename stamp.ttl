###  http://www.owlsai.com/ontologies/stamp#ControlledProcess
:ControlledProcess rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( <http://www.owlsai.com/ontologies/tinytop#Process>
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasSubject ;
                                                                owl:someValuesFrom [ owl:intersectionOf ( <http://www.owlsai.com/ontologies/tinytop#Entity>
                                                                                                          [ rdf:type owl:Restriction ;
                                                                                                            owl:onProperty <http://www.owlsai.com/ontologies/situation#isBearerOf> ;
                                                                                                            owl:someValuesFrom [ owl:intersectionOf ( <http://www.owlsai.com/ontologies/situation#Fluent>
                                                                                                                                                      [ rdf:type owl:Restriction ;
                                                                                                                                                        owl:onProperty <http://www.owlsai.com/ontologies/controlsystem#isEffectedBy> ;
                                                                                                                                                        owl:someValuesFrom [ owl:intersectionOf ( <http://www.owlsai.com/ontologies/controlsystem#ControlAction>
                                                                                                                                                                                                  [ rdf:type owl:Restriction ;
                                                                                                                                                                                                    owl:onProperty <http://www.owlsai.com/ontologies/tinytop#capabilityOf> ;
                                                                                                                                                                                                    owl:someValuesFrom <http://www.owlsai.com/ontologies/controlsystem#Controller>
                                                                                                                                                                                                  ]
                                                                                                                                                                                                ) ;
                                                                                                                                                                             rdf:type owl:Class
                                                                                                                                                                           ]
                                                                                                                                                      ]
                                                                                                                                                    ) ;
                                                                                                                                 rdf:type owl:Class
                                                                                                                               ]
                                                                                                          ]
                                                                                                        ) ;
                                                                                     rdf:type owl:Class
                                                                                   ]
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:comment "ControlledProcess is an abstraction used to group together entities whose Fluents are considered relevant, that are not themselves controllers, and are the \"subjects of\" the controlled process."@en ;
                   rdfs:label "Controlled Process"@en .


###  http://www.owlsai.com/ontologies/stamp#Hazard
:Hazard rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( <http://www.owlsai.com/ontologies/situation#Situation>
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://www.owlsai.com/ontologies/situation#hasPossible> ;
                                                     owl:someValuesFrom :Loss
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:comment "\"A situation in which it is possible for a loss to occur.\" (Source: STPA Handbook)"@en ;
        rdfs:label "Hazard"@en .


###  http://www.owlsai.com/ontologies/stamp#HazardousProcess
:HazardousProcess rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( <http://www.owlsai.com/ontologies/tinytop#Process>
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty <http://www.owlsai.com/ontologies/situation#hasResult> ;
                                                               owl:someValuesFrom :Hazard
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:comment "A Process that results in a Hazard"@en ;
                  rdfs:label "Hazardous Process"@en .


###  http://www.owlsai.com/ontologies/stamp#Loss
:Loss rdf:type owl:Class ;
      rdfs:subClassOf <http://www.owlsai.com/ontologies/tinytop#Capability> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.owlsai.com/ontologies/situation#isPossibleIn> ;
                        owl:allValuesFrom :Hazard
                      ] ;
      rdfs:comment "The Capability to transition to a LossMode, in which something of value to the stakeholders is lost."@en ;
      rdfs:label "Loss"@en .


###  http://www.owlsai.com/ontologies/stamp#NotProvidingPotentiallyHazardousControlAction
:NotProvidingPotentiallyHazardousControlAction rdf:type owl:Class ;
